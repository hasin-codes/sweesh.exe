{
  "name": "sweesh-electron-react",
  "version": "1.0.0",
  "description": "React + Electron app with TypeScript",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run build:main:watch\" \"npm run build:watch\" \"wait-on dist/main.js dist/renderer.js && electron .\"",
    "build": "webpack --mode production",
    "build:watch": "webpack --mode development --watch",
    "build:main": "tsc src/main.ts --outDir dist --target es2020 --module commonjs --esModuleInterop --skipLibCheck",
    "build:main:watch": "tsc src/main.ts --outDir dist --target es2020 --module commonjs --esModuleInterop --skipLibCheck --watch",
    "clean": "rimraf dist",
    "rebuild": "npm run clean && npm run build:main && npm run build"
  },
  "keywords": [
    "electron",
    "react",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.8.1",
    "electron": "^28.1.0",
    "electron-packager": "^17.1.2",
    "html-webpack-plugin": "^5.6.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "tailwindcss": "^4.1.14",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.546.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.3.1"
  }
}
